<form method="post" action="{{ path('app_cocktail_delete', {'id': cocktail.id}) }}" onsubmit="return confirm('Confirmer la suppression');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ cocktail.id) }}">
	<button class="trash-icon">
		<i class="bi bi-trash-fill me-2 icon"></i>
	</button>
</form>
